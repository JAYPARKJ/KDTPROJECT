
-- 회원 롤(들) 삭제
DELETE 
	FROM USER_ROLES
	WHERE USERNAME = 'abcd1111';
	
-- 회원정보 삭제 
DELETE
	FROM MEMBER_TBL
  WHERE ID = 'abcd1111';
  
SELECT * FROM MEMBER_TBL WHERE ID='abcdabcd1111';

SELECT *
  FROM USER_ROLES
 WHERE USERNAME = 'abcd1111';
 
 INSERT INTO
 	USER_ROLES
 	VALUES
 	(USER_ROLES_SEQ.NEXTVAL)
  
