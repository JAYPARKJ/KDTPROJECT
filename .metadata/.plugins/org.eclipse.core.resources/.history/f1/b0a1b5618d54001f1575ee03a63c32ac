
-- 회원 롤(들) 삭제
DELETE 
	FROM USER_ROLES
	WHERE USERNAME = 'mbc_1001';
	
-- 회원정보 삭제 
DELETE
	FROM MEMBER_TBL
  WHERE ID = 'mbc_1001';
  
SELECT * FROM MEMBER_TBL WHERE ID='mbc_10011001';

SELECT *
  FROM USER_ROLES
 WHERE USERNAME = 'mbc_1001';
 
 INSERT INTO
 	USER_ROLES
 	VALUES
 	(USER_ROLES_SEQ.NEXTVAL, 'mbc_1001', 'ROLE_ADMIN');
  
